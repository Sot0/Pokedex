{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[typescrypt]": {
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.formatOnSave": true
  },
  "emmet.includeLanguages": {
    "ejs": "html"
  },
  "files.associations": {
    "*.ejs": "html"
  },
  "htmlhint.options": {
    "spec-char-escape": false,
    "doctype-first": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.tsdk": "node_modules\\typescript\\lib"
}
